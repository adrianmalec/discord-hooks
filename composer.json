{
  "name": "adrianmalec/discord-hooks",
  "type": "library",
  "description": "Simple library to send awesome discord webhooks.",
  "keywords": [
    "discord",
    "webhook",
    "webhooks",
    "discord hook"
  ],
  "homepage": "https://github.com/adrianmalec/discord-hooks",
  "license": "MIT",
  "authors": [
    {
      "name": "Adrian Malec",
      "homepage": "http://github.com/adrianmalec"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5.2"
  },
  "autoload": {
    "psr-4": {
      "DiscordHooks\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DiscordHooks\\Tests\\": "tests/"
    }
  }
}
